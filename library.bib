	Keywords = {rubisco, evolution, kinetics, photosynthesis},
	Month = {02},
	Number = {8},
	Pages = {3475--3480},
	Title = {Cross-species analysis traces adaptation of Rubisco toward optimality in a low-dimensional landscape},
	Ty = {JOUR},
