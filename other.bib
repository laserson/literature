@article{Farci:2010,
	Abstract = {Hepatitis B virus (HBV)-associated acute liver failure (ALF) is a dramatic clinical syndrome due to a sudden loss of hepatic cells leading to multiorgan failure. The mechanisms whereby HBV induces ALF are unknown. Here, we show that liver tissue collected at the time of liver transplantation in two patients with HBV-associated ALF is characterized by an overwhelming B cell response apparently centered in the liver with massive accumulation of plasma cells secreting IgG and IgM, accompanied by complement deposition. We demonstrate that the molecular target of these antibodies is the hepatitis B core antigen (HBcAg); that these anti-bodies display a restricted variable heavy chain (V) repertoire and lack somatic mutations; and that these two unrelated individuals with ALF use an identical predominant V gene with unmutated variable domain () for both IgG and IgM anti-HBc antibodies, indicating that HBcAg is the target of a germline human V gene. These data suggest that humoral immunity may exert a primary role in the pathogenesis of HBV-associated ALF. },
	Annote = {10.1073/pnas.1003854107},
	Author = {Farci, Patrizia and Diaz, Giacomo and Chen, Zhaochun and Govindarajan, Sugantha and Tice, Ashley and Agulto, Liane and Pittaluga, Stefania and Boon, Denali and Yu, Claro and Engle, Ronald E. and Haas, Mark and Simon, Richard and Purcell, Robert H. and Zamboni, Fausto},
	Date = {2010/05/11},
	Date-Added = {2010-08-25 18:08:01 -0400},
	Date-Modified = {2010-08-25 18:08:01 -0400},
	Journal = {Proceedings of the National Academy of Sciences},
	Journal1 = {Proceedings of the National Academy of Sciences},
	Keywords = {liver, immunology, hepatitis, immune repertoire},
	Month = {05},
	Number = {19},
	Pages = {8766--8771},
	Title = {B cell gene signature with massive intrahepatic production of antibodies to hepatitis B core antigen in hepatitis B virus--associated acute liver failure},
	Ty = {JOUR},
	Url = {http://www.pnas.org/content/107/19/8766.abstract},
	Volume = {107},
	Year = {2010},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.pnas.org/content/107/19/8766.abstract}}

@article{Widder:2010,
	Abstract = {From bacteria to fish, a remarkable variety of marine life depends on bioluminescence (the chemical generation of light) for finding food, attracting mates, and evading predators. Disparate biochemical systems and diverse phylogenetic distribution patterns of light-emitting organisms highlight the ecological benefits of bioluminescence, with biochemical and genetic analyses providing new insights into the mechanisms of its evolution. The origins and functions of some bioluminescent systems, however, remain obscure. Here, I review recent advances in understanding bioluminescence in the ocean and highlight future research efforts that will unite molecular details with ecological and evolutionary relationships. },
	Annote = {10.1126/science.1174269},
