	Keywords = {virus dynamics, resistance, CRISPR, metagenomics},
	Month = {5},
	Number = {5879},
	Pages = {1047--1050},
	Read = {1},
	Title = {Virus Population Dynamics and Acquired Virus Resistance in Natural Microbial Communities},
	Ty = {JOUR},
